    "dependencies": {
        "babel-preset-react-app": "^3.0.3",
        "babel-runtime": "^6.26.0",
        "prop-types": "^15.6.0",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "react-redux": "^5.0.6",
        "react-router-dom": "^4.2.2",
        "redux": "^3.7.2",
        "redux-immutable-state-invariant": "^2.1.0",
        "redux-thunk": "^2.2.0",
        "styled-components": "^2.2.1"
